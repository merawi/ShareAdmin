            @Html.DisplayFor(modelItem => item.ShareholderName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ShareholderNameAmh)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NSharesSubscribed)
